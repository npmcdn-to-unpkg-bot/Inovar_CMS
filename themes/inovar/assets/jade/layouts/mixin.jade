- var newline = "\r\n"
- var loopIterator = '$iterator'



mixin if3(val1,val2,val3)
    | {% if #{val1} #{val2} '#{val3}' %}
    =newline + newline
mixin elseif3(val1,val2,val3)
    | {% elseif #{val1} #{val2} '#{val3}' %}
    =newline + newline


//- twig if, elseif, esle, endif
  Example:  +if('kenny.sick')+elseif('kenny.dead')+else()+endif()
  Compiled: {% if kenny.sick %}{% elseif kenny.dead %}{% else %}{% endif %}
mixin if(desc)
    | {% if #{desc} %}
    =newline + newline
mixin elseif(desc)
    | {% elseif #{desc} %}
    =newline + newline
mixin else()
    | {% else %}
    =newline + newline
mixin endif()
    | {% endif %}
    =newline + newline






//- twig info layout
  Example:  +infolayout('custom description')
  Compiled: description = custom description
mixin infofile(desc)
    | description = "#{desc}"
    =newline + newline


//- twig end info file
  Example:  +endinfofile()
  Compiled: ==
mixin endinfofile()
    =newline
    | ==
    =newline


//- twig info page
  Example:  +infopage('Title page','/','default',Custom description'})
  Compiled: title = "Demonstration"
            url = "/"
            layout = "default"
            description = ""
            ==
mixin infopage(title,url,layout,description)
    | title = "#{title}"
    =newline
    | url = "#{url}"
    =newline
    | layout = "#{layout}"
    =newline
    | description = "#{description}"
    =newline + newline

//- twig info page
  Example:  +inportfileinfo('title')
  Compiled: {{ this.page.title }}
mixin inportfileinfo(info)
    | {{ this.page.#{info} }}

//- twig partial
  Example:  +partial('home/base')
  Compiled: {% partial "home/base" %}
mixin partial(info)
    =newline
    | {% partial "#{info}" %}



//- twig yield
  Example:  +yield('pages')
  Compiled: {% placeholder pages %}
mixin yield(info)
    =newline
    =newline
    | {% placeholder #{info} %}
    =newline
    =newline

//- twig section
  Example:  +section('styles')
  Compiled: {% put content %}
mixin section(info)
    =newline
    =newline
    | {% put #{info} %}
    =newline
//- twig endsection
  Example:  +endsection()
  Compiled: {% endput %}
mixin endsection()
    =newline
    | {% endput %}
    =newline
    =newline

//- twig content
  Example:  +content('pages/home.htm')
  Compiled: {% content "pages/home.htm" %}
mixin content(info)
    =newline
    =newline
    | {% content "#{info}" %}
    =newline
    =newline

//- twig component
  Example:  +component('blogPosts')
  Compiled: {% component "blogPosts" %}
mixin component(info)
    =newline
    =newline
    | {% component "#{info}" %}
    =newline
    =newline


//- twig component
  Example:  +editable('welcome.htm')
  Compiled: {% component 'editable' file="welcome.htm" %}
mixin editable(info)
    =newline
    =newline
    | {% component 'editable' file="#{info}" %}
    =newline
    =newline


//- twig info component
  Example:  +infocomponent('blogPosts')
  Compiled: [blogPosts]
mixin infocomponent(info)
    =newline
    =newline
    | [#{info}]
    =newline


//- twig generic option
  Example:  +optiongenericcomponent('slug','"{{ :slug }}"')
  Compiled:   slug = "{{ :slug }}"
mixin optiongenericcomponent(val1,val2)
    | #{val1} = #{val2}
    =newline


//- twig component option
  Example:  +optionblogcomponent('page','','10','No posts found','published_at desc','blog/category','blog/post')
  Compiled:   pageNumber = "{{ :page }}"
              categoryFilter =
              postsPerPage = 10
              noPostsMessage = "No posts found"
              sortOrder = "published_at desc"
              categoryPage = "blog/category"
              postPage = "blog/post"
mixin optionblogcomponent(val1,val2,val3,val4,val5,val6,val7)
    | pageNumber = "{{ :#{val1} }}"
    =newline
    | categoryFilter = "{{ #{val2} }}"
    =newline
    | postsPerPage = #{val3}
    =newline
    | noPostsMessage = "#{val4}"
    =newline
    | sortOrder = "#{val5}"
    =newline
    | categoryPage = "#{val6}"
    =newline
    | postPage = "#{val7}"
    =newline
    =newline

